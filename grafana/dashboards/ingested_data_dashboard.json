{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true,
        "hide": true
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "iteration": 165,
  "links": [],
  "panels": [
    {
      "datasource": "${POSTGRES_DB}",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.3 },
              { "color": "red", "value": 0.7 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS') AS time,\n  COUNT(*) FILTER (WHERE NOT is_critical) * 100.0 / COUNT(*) AS value\nFROM validation_statistics\nWHERE $__timeFilter(to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS'))\nGROUP BY 1\nORDER BY 1",
          "refId": "A"
        }
      ],
      "title": "‚úÖ % Valid Data Samples (last 10 min)",
      "type": "gauge"
    },
    {
      "datasource": "${POSTGRES_DB}",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT\n  jsonb_object_keys(issues_found::jsonb) AS category,\n  COUNT(*) AS count\nFROM validation_statistics\nWHERE $__timeFilter(to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS'))\nGROUP BY category\nORDER BY count DESC",
          "refId": "A"
        }
      ],
      "title": "üß™ Number of Detected Issues Per Category",
      "type": "table"
    },
    {
      "datasource": "${POSTGRES_DB}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "id": 3,
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS') AS time,\n  COUNT(*) FILTER (WHERE is_critical) AS critical,\n  COUNT(*) FILTER (WHERE NOT is_critical) AS valid\nFROM validation_statistics\nWHERE $__timeFilter(to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS'))\nGROUP BY 1\nORDER BY 1",
          "refId": "A"
        }
      ],
      "title": "üïê Valid vs Invalid Data Over Time",
      "type": "timeseries"
    },
    {
      "datasource": "${POSTGRES_DB}",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 2 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "id": 4,
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS') AS time,\n  COUNT(*) AS value\nFROM validation_statistics\nWHERE is_critical = true AND $__timeFilter(to_timestamp(\"timestamp\", 'YYYY-MM-DD\"T\"HH24:MI:SS.MS'))\nGROUP BY 1\nORDER BY 1",
          "refId": "A"
        }
      ],
      "title": "‚ö†Ô∏è Number of Critical Files",
      "type": "stat"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 37,
  "style": "dark",
  "tags": ["data", "ingestion", "validation"],
  "templating": { "list": [] },
  "time": { "from": "now-30m", "to": "now" },
  "timepicker": { "refresh_intervals": ["10s", "30s", "1m"] },
  "timezone": "",
  "title": "üì• Ingested Data Monitoring Dashboard",
  "version": 2
}
